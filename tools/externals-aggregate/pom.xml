    <dependency>
      <groupId>de.maggu2810.shk.tools.externals</groupId>
      <artifactId>apache-commons</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
    </dependency>

    <!-- Apache Felix -->
    <dependency>
      <groupId>de.maggu2810.shk.tools.externals</groupId>
      <artifactId>apache-felix</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
    </dependency>

    <!-- Apache Xbean -->
    <dependency>
      <groupId>de.maggu2810.shk.tools.externals</groupId>
      <artifactId>apache-xbean</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
    </dependency>

    <!-- Eclipse Equinox -->
    <dependency>
      <groupId>de.maggu2810.shk.tools.externals</groupId>
      <artifactId>eclipse-equinox</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
    </dependency>

