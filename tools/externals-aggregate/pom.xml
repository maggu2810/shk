    <dependency>
      <groupId>de.maggu2810.shk.tools.externals</groupId>
      <artifactId>xtext</artifactId>
      <version>${project.version}</version>
      <type>pom</type>
      <exclusions>
        <exclusion>
          <groupId>com.google.guava</groupId>
          <artifactId>guava</artifactId>
        </exclusion>
      </exclusions>
